<h1>Edit Duck Form</h1>

<%= form_for @duck do |f|%>
<p><%= f. label :name %></p>
<p><%= f.text_field :name %></p>
<p><%= f.label :description %></p>
<p><%= f.text_field :description %></p>
<p><%= f.label :student_id %></p>
<p><%= f.collection_select :student_id, Student.all, :id, :name %></p>
<p><%= f.submit "Submit Edited Duck"%></p>
<% end %>